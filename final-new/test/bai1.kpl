PROGRAM  EXAMPLE1;  (* Example 1 *)
CONST MAX = 10;
TYPE T = INTEGER;
VAR  A : ARRAY(. 3 .) OF T;
     N : INTEGER;
     CH : CHAR;

PROCEDURE INPUT;
VAR I : INTEGER;
    TMP : INTEGER;
BEGIN
    A(.1.) := 1;
    CALL WRITEI(A(.1.));
    A(.2.) := 5;
    CALL WRITEI(A(.2.));
    A(.3.) := 3;
    CALL WRITEI(A(.3.));
END;

PROCEDURE CHECK2;
VAR I: INTEGER;
BEGIN
    If A(.1.) >= A(.3.) Then CALL WRITEI(A(.3.)) Else CALL WRITEI(A(.1.));
END;


PROCEDURE CHECK1;
VAR I: INTEGER;
BEGIN
    If A(.2.) >= A(.3.) Then CALL WRITEI(A(.2.)) Else CALL CHECK2;
END;

PROCEDURE CHECK4;
VAR I: INTEGER;
BEGIN
    If A(.1.) >= A(.3.) Then CALL WRITEI(A(.1.)) Else CALL WRITEI(A(.3.));
END;

PROCEDURE CHECK3;
VAR I: INTEGER;
BEGIN
    If A(.2.) <= A(.3.) Then CALL WRITEI(A(.2.)) Else CALL CHECK4;
END;

PROCEDURE OUTPUT;
VAR I: INTEGER;
BEGIN
    If A(.1.) >= A(.2.) Then  CALL CHECK1 Else CALL CHECK3 ;
END;

BEGIN
    CALL INPUT;
    CALL OUTPUT;
END.  (* Example 1 *)
